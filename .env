registry=exadel/
postgres_username=postgres
postgres_password=postgres
postgres_db=frs
postgres_domain=compreface-postgres-db
postgres_port=5432
email_host=smtp.gmail.com
email_username=
email_from=
email_password=
enable_email_server=false
save_images_to_db=true
compreface_api_java_options=-Xmx4g
compreface_admin_java_options=-Xmx1g
max_file_size=5MB
max_request_size=10M
max_detect_size=640
uwsgi_processes=3
uwsgi_threads=1
connection_timeout=10000
read_timeout=60000
ADMIN_VERSION=1.2.0
API_VERSION=1.2.0
FE_VERSION=1.2.0
CORE_VERSION=1.2.0
POSTGRES_VERSION=1.2.0

## TensorFlow / GPU defaults (used by embedding-calculator)
# GPU index for services (-1 disables GPU)
GPU_IDX=0
# CUDA version shorthand (used by MXNet selection and for documentation). Keep in sync with host CUDA, e.g. 11.7
CUDA=11.8
# TensorFlow version for embedding-calculator
TF_VERSION=2.12.0
# Which pip package to install for TensorFlow inside the container (set to tensorflow-gpu if you use that wheel)
TENSORFLOW_PACKAGE=tensorflow-gpu
# Control device visibility inside container
CUDA_VISIBLE_DEVICES=0
NVIDIA_VISIBLE_DEVICES=all
NVIDIA_DRIVER_CAPABILITIES=compute,utility
